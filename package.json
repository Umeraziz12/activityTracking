{
  "name": "vscode-time-tracker",
  "displayName": "ActivityTracker",
  "description": "Track time spent on projects and files.",
  "version": "0.0.1",
  "publisher": "umer-Aziz",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["*"],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "umerAziz.helloWorld",
      "title": "ActivityTracker"
    },
    {
      "command": "umerAziz.toggleTracking",
      "title": "Toggle Tracker"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.97.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.19.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
